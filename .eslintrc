{
  "extends": [
    "connertech"
  ],
  "overrides": [
    {
      "files": [
        "test/**/*.js"
      ],
      "rules": {
        "spaced-comment": "off",
        "func-names": "off",
        "no-undef": "off",
        "no-unused-expressions": "off",
        "import/no-extraneous-dependencies": "off"
      },
      "env": {
        "jest": true
      }
    }
  ]
}
